function isAnagramOfPalindrome(str) {
    const charCount = {};
    let oddCount = 0;
  
    for (let i = 0; i < str.length; i++) {
      const char = str[i];
      charCount[char] = (charCount[char] || 0) + 1;
    }
  
    for (let count of Object.values(charCount)) {
      if (count % 2 !== 0) {
        oddCount++;
      }
      if (oddCount > 1) {
        return false;
      }
    }
  
    return true;
  }
  
